<body>

<!--抬头开始-->
<%@include file="top.jsp" %>

<!--抬头结束-->
<!--会员注册开始-->

<div class="huiyuan_zc">
<div class="zhuce_1">用户注册</div>
<div class="zhuce_2">
<form action="UserServlet?method=register" method="post">
<div class="zhuce_left">


<div Style="position: relative;left:200px;">
<table>
<tr>
<td><div class="yhm_zc_1">用户名：</div></td>
<td><div class="yhm_zc"><input type="text" name="username"  class="zc_wbk" /><span id="check_name"></span></div></td>	
</tr>
<tr>
<td><div class="yhm_zc_1">输入密码：</div></td>
<td><div class="yhm_zc"><input type="text" name="password"  class="zc_wbk" /></div></td>	
</tr>
<tr>
<td><div class="yhm_zc_1">再次输入：</div></td>
<td><div class="yhm_zc"><input id="test" type="text" name="test"  class="zc_wbk" /></div></td>	

</tr>
<tr>
<td><div class="yhm_zc_1">地址：</div></td>
<td><div class="yhm_zc"><input type="text" name="address"  class="zc_wbk" /></div></td>	
</tr>
<tr>
<td><div class="yhm_zc_1">联系电话：</div></td>
<td><div class="yhm_zc"><input type="text" name="phone"  class="zc_wbk" /></div></td>	
</tr>

<tr>
<td colspan="2"><div class="yhm_zc_1"><input type="submit" style="width:200px;height:30px;background: url('images/zhuce_1.jpg');position: relative;left:100px;cursor: pointer;" value=""/>
</div>
</td>
<td><a href="denglu.jsp"><input type="button" style="width:100px;height:30px;background: url('images/dll.jpg');background-repeat:no-repeat; relative;left:50px;cursor: pointer;" value=""/></a></td>
</tr>

</table>
</div>
<!-- <img src="images/dll.jpg" width="149" height="78" / style="margin-left:50px; margin-top:20px;"> -->
</form>

</div>
</div>

</div>


<!--中部开始-->


<div class="con_ny">




</div>


<!--中部结束-->

<%@include file="bottom.jsp" %>

<script>
	$(document).ready(function(){
		
		$("[name='username']").blur(function(){
			
			$.get("UserServlet?method=checkName&checkname="+$(this).val(),function(data,status){
					
					if(status=='success'){
					if(data=='true'){
						$("#check_name").attr("style","display:inline");
						$("#check_name").html("<b style='color:red'>×</b>");
					}
					else{
						$("#check_name").attr("style","display:inline");
						 $("#check_name").html("<b style='color:green'>√</b>"); 
					}
				}
			});
			
		});
		$("[name='username']").focus(function(){
			$(this).val("");
			$("#check_name").attr("style","display:none");
		});
	});
	

</script>

<c:if test="${requestScope.mess != null}">
	<script>
		alert("${requestScope.mess}");
	</script>  
</c:if>
<c:if test="${requestScope.zhuce !=null }">
<c:choose>
   <c:when test="${requestScope.zhuce==true }">
   <script>
		alert("注册成功！");
	</script>  
   </c:when>
   <c:otherwise> 
   <script>
		alert("注册失败！");
	</script>
   </c:otherwise>
</c:choose>
</c:if>
</body>
