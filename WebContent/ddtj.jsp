<!--中部开始-->
<div style="margin: auto;position: relative;left:450px;">
<table width="0" border="0" cellspacing="0" cellpadding="0">
  <tr style=" font-family:微软雅黑; text-align:center; font-size:14px;">
    <td width="69"><input name="" type="checkbox" value="" / style=" margin-left:10px; margin-right:10px;">全选</td>
    <td width="279">商品信息</td>
    <td width="116">单价（元）</td>
    <td width="114">特惠（元）</td>
    <td width="147">数量</td>
    <td width="118">小计（元）</td>
    <td width="72">操作</td>
  </tr>
  <c:forEach var="wine" items="${sessionScope.collects }">
  <tr style=" font-family:微软雅黑; text-align:center; font-size:14px;">
    <td width="43" height="87"><input name="" type="checkbox" value=""  style=" margin-left:10px; margin-right:10px;"></td>
    <td width="305"><span class="ddtp_3"><img src="${wine.key.path }" width="59" height="58" /></span>
    <span class="ddmc_xq"><a href="#">${wine.key.wine_name }&nbsp;${wine.key.kind }</a></span></td>
    <td width="116">￥${wine.key.price}</td>
    <td width="114" style="color:#F00;">￥${fn:substring(wine.key.price*0.8,0,3)}</td>
    <td width="147">
    <input name="" value="-" type="button"  style="background: url(images/jia.jpg); width:19px; height:19px;" onclick="jian()">
	<input id="shu" type="text" value="${wine.value }"  style="width:40px; text-align:center; height:18px; border:1px solid #dddddd;" >  
	<input name="" value="+" type="button"  style="background: url(images/jia.jpg); width:19px; height:19px;" onclick="jia()">
	</td>
    <td width="118" style="color:#F00;">￥${fn:substring(wine.key.price*0.8,0,5)}</td>
    <td width="72"><a href="#">删除</a></td>
  </tr>
  </c:forEach>
</table>
